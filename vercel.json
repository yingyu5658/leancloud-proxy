{
  "routes": [
    {
      "src": "/leancloud/(.*)",
      "dest": "https://ob3nrzua.api.lncldglobal.com/1.1/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,X-LC-Id,X-LC-Key"
      }
    },
    {
      "src": "/.*",
      "status": 404,
      "headers": { "Cache-Control": "no-store" }
    }
  ],
  "headers": {
    "User-Agent": "Mozilla/5.0 (compatible; Valine-Proxy/1.0; +https://github.com/yingyu5658/leancloud-proxy/)"
  }
}
